<div>
    <h1 style = "color:white">새 메일 작성</h1>
    <%= form_for :article, url: '/articles/' do |f| %> 
    <p>
        <%= f.label :title %> 
        <%= f.text_field :title %> 
    </p>
    <p>
        <%= f.label :send %> 
        <%= f.text_field :send %> 
    </p>
    <p>
        <%= f.label :content %><br>
        <%= f.text_field :content %> 
    </p>
    
    <p>
        <%= f.submit %>
    </p>
</div>